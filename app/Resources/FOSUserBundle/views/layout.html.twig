<!DOCTYPE html>
<html class="fuelux">

    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}SELF{% endblock %}</title>
        <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/boodtstrap.min.css" rel="stylesheet">
        <link href="{{ asset("bundles/innovaself/css/fuelux.css") }}" rel="stylesheet" />
        <link href="http://www.fuelcdn.com/fuelux-imh/2.4.0/css/fuelux-responsive.css" rel="stylesheet" />
        <link href="{{ asset("bundles/innovaself/css/main.css") }}" rel="stylesheet" />
    </head>

    <body>
        <div class="container">
            <div class="row">
                <div class="span12">
                    {# TODO: BR to remove !!!!#}
                    <br/>
                    {% for type, messages in app.session.flashbag.all() %}
                        {% for key, message in messages %}
                            <div class="flash-{{ type }}">
                                <div class="alert alert-success">{{ message|trans({}, 'FOSUserBundle') }}</div>
                            </div>
                        {% endfor %}
                    {% endfor %}

                    <div>
                        {% block fos_user_content %}
                        {% endblock fos_user_content %}
                    </div>
                </div>
            </div>
        </div>
        <!-- Jquery first then Bootstrap call -->
        <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
        <script src="http://www.fuelcdn.com/fuelux/2.4.1/loader.min.js" type="text/javascript"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        <script type="text/javascript" src="{{ asset("bundles/innovaself/js/main.js") }}"></script>
    </body>

</html>