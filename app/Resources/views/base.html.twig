                    <li><a href="{{ path('fos_user_security_logout') }}">Logout</a></li>
                </ul>
            </div>
        </nav>
        <div class="container">
            <div class="col-md-12">
                {% block body %}{% endblock %}
            </div>
        </div>
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
        {% block javascripts %}{% endblock %}
